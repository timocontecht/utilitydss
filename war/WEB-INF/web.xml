<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- Servlets -->
   <!-- Servlets -->
  <servlet>
    <servlet-name>utilityDSSServlet</servlet-name>
    <servlet-class>org.visico.utilitydss.server.UtilityDSSServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>utilityDSSServlet</servlet-name>
    <url-pattern>/utilitydss/utilitydss</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>UtilityDSS.html</welcome-file>
  </welcome-file-list>

</web-app>
